<?xml version="1.0" encoding="UTF-8"?>
<area
	id="area"
	title="系统告警统计图"
	showTitle="true"
	xUnit="日期"
	xScale="T_WARN"
	xScaleWidth="1"
	xScaleIsShow="true"
	showShadow="false"
	interval="6"
	isStack="false"
	yUnit="人数"
	minY="0"
>
	<titleStyle color="#555555" fontStyle="normal" fontWeight="200" fontFamily="微软雅黑" fontSize="18"/>
	<data>
		<dataSource>
			<![CDATA[
				SELECT * FROM WM_INTE_WARN WHERE T_WARN > TO_DATE('20170715','yyyymmdd')
          	 ]]>
		</dataSource>
		<dataDetail>
			<chart name="告警次数" field="N_TIMES" colorLine="#33CC99" colorArea="#33CC99"/>
		</dataDetail>
	</data>
</area>